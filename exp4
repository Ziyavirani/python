class Student:
    def __init__(self, name, age, roll_no):
        self.name = name
        self.age = age
        self.roll_no = roll_no
        
    def display(self):
        print("Name:", self.name)
        print("Age:", self.age)
        print("Roll No:", self.roll_no)

print("Student 1:")
s1 = Student("John", 23, 101)
s1.display()

print("\nStudent 2:")
s2 = Student("Jane", 22, 102)
s2.display()
